{
    "name": "intellect-web-development/symfony-entry-contract-bundle",
    "description": "SymfonyEntryContract Symfony bundle for pretty contracts",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Erin",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/config": "*",
        "symfony/dependency-injection": "*",
        "symfony/http-kernel": "*",
        "symfony/validator": "^6.3",
        "symfony/serializer": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3",
        "phpstan/phpstan": "^1.10",
        "vimeo/psalm": "^5.15",
        "overtrue/phplint": "^9.0",
        "friendsofphp/php-cs-fixer": "^3.23",
        "rector/rector": "^0.18.0",
        "symfony/test-pack": "^1.0",
        "symfony/framework-bundle": "^6.3",
        "symfony/phpunit-bridge": "^6.3",
        "zircote/swagger-php": "^4.7"
    },
    "autoload": {
        "psr-4": {
            "IWD\\SymfonyEntryContract\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IWD\\SymfonyEntryContract\\": "tests/"
        }
    }
}
